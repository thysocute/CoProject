h5 {
	color: #333;
}

hr {
	margin: 32px 0px;
	/*设置hr的颜色*/
	height: 1px;
 	background-color: #ddd;
  	border: none;
}

.form-group>label {
	display: inline-block;
	width: 100px;
	text-align: right;
	font-size: 1rem;
	padding-right: 10px;
}

input[type=text]:not(.browser-default) {
	width: 70%;
	border-color: #ddd;
}

.form-group .btn {
	width: 120px;
	height: 45px;
	margin: 50px 100px;
	font-size: 1.2rem;
}